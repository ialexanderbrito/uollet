{
  "name": "finance",
  "version": "3.9.0",
  "type": "module",
  "engines": {
    "node": ">=18.18.2",
    "npm": ">=8.1.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "format:check": "prettier --check src/",
    "commit": "cz",
    "release": "semantic-release --no-ci",
    "prepare": "husky install",
    "update-deps": "yarn && yarn build",
    "rmv": "rimraf node_modules && rimraf dist && rimraf yarn.lock"
  },
  "dependencies": {
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@hcaptcha/react-hcaptcha": "^1.9.3",
    "@headlessui/react": "^1.7.18",
    "@phosphor-icons/react": "^2.0.15",
    "@supabase/supabase-js": "^2.39.3",
    "@uiball/loaders": "^1.3.1",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "clsx": "^2.1.0",
    "configcat-react": "^4.3.0",
    "credit-card-type": "^10.0.0",
    "crisp-sdk-web": "^1.0.21",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.3.1",
    "emoji-mart": "^5.5.2",
    "formik": "^2.4.5",
    "kbar": "0.1.0-beta.45",
    "mathjs": "^12.3.2",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-auth-code-input": "^3.2.1",
    "react-chartjs-2": "^5.2.0",
    "react-color": "^2.19.3",
    "react-currency-input-field": "^3.6.14",
    "react-currency-mask": "^1.3.1",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.22.0",
    "react-tooltip": "^5.26.0",
    "react-web-share": "^2.0.2",
    "swiper": "^11.0.6",
    "tailwind-merge": "^2.2.1",
    "tailwind-scrollbar-hide": "^1.1.7",
    "xlsx": "^0.18.5",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@ialexanderbrito/eslint-config": "^2.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.11.17",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.55",
    "@types/react-color": "^3.0.11",
    "@types/react-dom": "^18.2.19",
    "@types/react-dropzone": "^5.1.0",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "semantic-release": "^22.0.12",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vite-plugin-pwa": "^0.18.0",
    "vite-plugin-radar": "^0.9.3",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint-staged"
    }
  }
}
